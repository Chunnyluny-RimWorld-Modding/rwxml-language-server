{
  "configurations": [
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Launch Client",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}"
      ],
      "env": {
        "languageServerEntryPath": "../language-server/dist/index.js"
      },
      "outFiles": [
        "${workspaceRoot}/dist/**/*.js"
      ]
    },
  ]
}