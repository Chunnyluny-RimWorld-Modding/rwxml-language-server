{
  "displayName": "RWXML language server",
  "name": "rwxml-language-server",
  "version": "0.4.28",
  "description": "Language server for rimworld XML modding, editing. support custom mods.",
  "main": "dist/client/index.js",
  "scripts": {
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "test": "jest",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore . --fix",
    "webpack": "webpack --mode development",
    "vscode:prepublish": "npm run upload-only && webpack --mode production"
  },
  "author": "madeline",
  "contributors": [
    "madeline"
  ],
  "license": "ISC",
  "engines": {
    "vscode": "^1.47.0"
  },
  "publisher": "madeline",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.159",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.3.0",
    "prettier": "^2.3.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.3",
    "typescript": "^3.9.4",
    "webpack": "^5.11.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "linq-es2015": "^2.5.1",
    "lodash": "^4.17.19",
    "tsconfig-paths": "^3.9.0",
    "vscode-languageclient": "^6.1.3",
    "vscode-languageserver": "^6.1.1",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-languageserver-types": "^3.15.1",
    "vscode-nls": "^4.1.2",
    "vscode-uri": "^2.1.2"
  }
}
